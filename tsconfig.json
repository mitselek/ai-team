{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "~/*": ["./app/*"],
      "@/*": ["./app/*"],
      "~/types/*": ["./types/*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"],
      "#app": ["./node_modules/nuxt/dist/app"],
      "#app/*": ["./node_modules/nuxt/dist/app/*"],
      "#imports": ["./.nuxt/imports"]
    }
  }
}
